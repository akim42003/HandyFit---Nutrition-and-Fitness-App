{
  "expo": {
    "name": "HandyFit",
    "slug": "HandyFit",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon-ios-optimized.png",
    "scheme": "handyfit",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.akim42003.HandyFit",
      "appleTeamId": "5JG9GHX4G6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-ios-optimized.png",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [],
      "edgeToEdgeEnabled": true,
      "package": "com.akim42003.HandyFit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-ios-optimized.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "537017f9-299d-4189-8752-66ab432d7a5e"
      }
    }
  }
}
